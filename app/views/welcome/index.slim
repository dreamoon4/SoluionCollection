h1 Solution Collection

form.form
  .input-group
    input.searchbox.form-control#srh placeholder='Search Problems...'
    span.input-group-addon
      i.glyphicon.glyphicon-search

#search-result-box

css:
  .sol-sticker {
    min-height: 64px;
    min-width: 64px;
    height: 64px;
    width: 64px;
  }
  .search-result {
    min-height: 100px;
  }
  .search-result:nth-child(even) {
    background-color: #EEE;
  }
  .search-template {
    display: none;
  }
